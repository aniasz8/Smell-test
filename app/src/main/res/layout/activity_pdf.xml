<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="9921px"
    android:layout_height="match_parent"
    android:id="@+id/pdf_layout"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#fef9cc"
    tools:context="com.example.anna.sniffin_sticks.pdfActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/pdf_data_day"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/pdf_data_patient"
        android:layout_below="@+id/pdf_data_day"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/pdf_data_result"
        android:layout_below="@+id/pdf_data_patient"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/pdf_data_points"
        android:layout_below="@+id/pdf_data_result"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/pdf_data_type"
        android:layout_below="@+id/pdf_data_points"
        android:layout_alignParentStart="true" />

    <TableLayout
        android:id = "@+id/RHE"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/pdf_data_type"
        android:layout_alignParentStart="true"
        android:layout_weight="0"
        android:padding="5dp">

        <TableRow android:layout_height="wrap_content">
            <TextView
                android:id="@+id/runLabel"
                android:text="Orange"
                android:layout_height="wrap_content"
                />
            <TextView
                android:id="@+id/hitLabel"
                android:text="Brombeere"
                android:layout_height="wrap_content"
                />
            <TextView
                android:id="@+id/errorLabel"
                android:text="Erdbeere"
                android:layout_height="wrap_content"
                />
            <TextView
                android:id="@+id/errorLabel1"
                android:text="Ananas"
                android:layout_height="wrap_content"
                />
        </TableRow>

        <TableRow android:layout_height="wrap_content">
        <TextView
            android:id="@+id/visitorRuns"
            android:text="0"
            android:layout_height="wrap_content"
            />
        <TextView
            android:id="@+id/visitorHits"
            android:text="0"
            android:layout_height="wrap_content"
            />
        <TextView
            android:id="@+id/errorLabel2"
            android:text="Ananas"
            android:layout_height="wrap_content"
            />
        <TextView
            android:id="@+id/errorLabel3"
            android:text="Ananas"
            android:layout_height="wrap_content"
            />
        </TableRow>
    </TableLayout>
</RelativeLayout>
